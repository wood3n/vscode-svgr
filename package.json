{
  "name": "vscode-svgr",
  "version": "1.0.0",
  "license": "MIT",
  "displayName": "vscode-svgr",
  "publisher": "wood3n",
  "description": "vscode svgr extension",
  "author": {
    "name": "wood3n",
    "email": "wangkka1@163.com",
    "url": "https://icodex.me"
  },
  "keywords": [
    "vscode",
    "vscode extension",
    "svgr"
  ],
  "homepage": "https://github.com/wood3n/vscode-svgr#readme",
  "bugs": {
    "url": "https://github.com/wood3n/vscode-svgr/issues",
    "email": "wangkka1@163.com"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-svgr.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-svgr.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.56",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "eslint-plugin-oxygen": "^1.0.5",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@svgr/core": "^6.3.1",
    "@svgr/plugin-jsx": "^6.3.1",
    "@svgr/plugin-prettier": "^6.3.1",
    "@svgr/plugin-svgo": "^6.3.1",
    "lodash-es": "^4.17.21"
  }
}
